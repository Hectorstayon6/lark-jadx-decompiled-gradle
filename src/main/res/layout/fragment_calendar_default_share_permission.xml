        <View android:background="@color/line_divider_default" android:layout_width="match_parent" android:layout_height="@dimen/dp_05" android:layout_marginTop="@dimen/dp_8"/>
        <RelativeLayout android:id="@+id/tenantContainer" android:background="@color/bg_body" android:layout_width="match_parent" android:layout_height="wrap_content">
