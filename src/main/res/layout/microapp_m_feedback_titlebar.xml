<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/microapp_m_titleBar_content" android:background="#ffffff" android:layout_width="match_parent" android:layout_height="44dp">
    <LinearLayout android:gravity="bottom" android:id="@+id/microapp_m_page_close_layout" android:paddingLeft="13dp" android:layout_width="wrap_content" android:layout_height="44dp" android:layout_alignParentBottom="true">
        <ImageButton android:layout_gravity="center_vertical" android:id="@+id/microapp_m_page_back" android:background="@drawable/microapp_m_lefterbackicon_titlebar_light" android:layout_width="24dp" android:layout_height="24dp" android:layout_marginRight="11dp"/>
        <ImageButton android:layout_gravity="center_vertical" android:id="@+id/microapp_m_page_close" android:background="@drawable/microapp_m_leftercloseicon_titlebar_light" android:visibility="gone" android:layout_width="24dp" android:layout_height="24dp"/>
    </LinearLayout>
    <TextView android:textSize="17sp" android:textColor="#222222" android:ellipsize="end" android:gravity="center" android:id="@+id/microapp_m_page_title" android:layout_width="wrap_content" android:layout_height="44dp" android:layout_marginRight="190dp" android:singleLine="true" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" android:layout_centerVertical="true"/>
    <TextView android:enabled="false" android:textSize="16sp" android:textStyle="bold" android:textColor="@color/microapp_m_feedback_send_text_color_unavailable" android:gravity="center" android:id="@+id/microapp_m_feedback_send" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="44dp" android:layout_marginRight="15dp" android:text="@string/microapp_m_feedback_faq_commit_send" android:layout_alignParentRight="true" android:layout_alignParentBottom="true" android:layout_centerVertical="true"/>
    <View android:background="@color/microapp_m_dark_divider" android:layout_width="match_parent" android:layout_height="0.25dp" android:layout_alignParentBottom="true"/>
</RelativeLayout>
