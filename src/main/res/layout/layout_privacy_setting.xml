<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:gravity="center_vertical" android:id="@+id/privacy_setting" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/mine_setting_item_height">
    <LinearLayout android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/setting_item_text_margin_left" android:layout_weight="1">
        <TextView android:textSize="16sp" android:textColor="@color/text_title" android:id="@+id/display_text" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        <TextView android:textSize="14sp" android:textColor="@color/text_placeholder" android:id="@+id/display_subtext" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    </LinearLayout>
    <com.larksuite.component.ui.button.LKUISwitchButton android:id="@+id/switch_button" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
</LinearLayout>
