<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="vertical" android:background="@color/lkui_N100" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.ss.android.lark.ui.CommonTitleBar android:id="@+id/titlebar" android:layout_width="match_parent" android:layout_height="wrap_content" app:tb_title="@string/Lark_NewSettings_Privacy" app:tb_title_color="@color/lkui_N900"/>
    <include layout="@layout/view_title_privacy_setting_desktop"/>
    <com.ss.android.lark.contact.impl.setting.widget.SwitchSettingView android:id="@+id/view_setting_ding" android:background="@drawable/settings_item_bg_desktop_selector" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginTop="@dimen/setting_group_margin" android:layout_marginRight="16dp"/>
    <LinearLayout android:orientation="vertical" android:id="@+id/layout_find_me" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginTop="@dimen/setting_group_margin" android:layout_marginRight="16dp">
        <TextView android:textSize="12sp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/Lark_NewSettings_FindMeVia" style="@style/SettingGroupTitle"/>
        <com.ss.android.lark.contact.impl.setting.widget.FindMeSettingView android:orientation="vertical" android:id="@+id/view_find_me" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout android:orientation="vertical" android:id="@+id/layout_add_me" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginTop="@dimen/setting_group_margin" android:layout_marginRight="16dp">
        <TextView android:textSize="12sp" android:id="@+id/text_add_way" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/Lark_NewSettings_AddMeVia" style="@style/SettingGroupTitle"/>
        <com.ss.android.lark.contact.impl.setting.widget.SwitchSettingView android:id="@+id/view_setting_name_card" android:background="@drawable/settings_item_bg_desktop_upper_selector" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/mine_setting_item_height_desktop"/>
        <com.ss.android.lark.setting.SettingDividerView android:id="@+id/view_divider_name_card" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/setting_divider_height" style="@style/SettingDividerLeftBlank"/>
        <com.ss.android.lark.contact.impl.setting.widget.SwitchSettingView android:id="@+id/view_setting_token" android:background="@drawable/settings_item_bg_desktop_lower_selector" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/mine_setting_item_height_desktop"/>
    </LinearLayout>
    <LinearLayout android:orientation="vertical" android:id="@+id/layout_add_me_scene" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginTop="@dimen/setting_group_margin" android:layout_marginRight="16dp">
        <TextView android:textSize="12sp" android:id="@+id/text_add_scene" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="4dp" android:text="@string/Lark_NewSettings_AddMeFrom" style="@style/SettingGroupTitle"/>
        <com.ss.android.lark.contact.impl.setting.widget.SwitchSettingView android:id="@+id/view_setting_chat" android:background="@drawable/settings_item_bg_desktop_upper_selector" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/mine_setting_item_height_desktop"/>
        <com.ss.android.lark.setting.SettingDividerView android:id="@+id/view_divider_chat" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/setting_divider_height" style="@style/SettingDividerLeftBlank"/>
        <com.ss.android.lark.contact.impl.setting.widget.SwitchSettingView android:id="@+id/view_setting_schedule" android:background="@drawable/settings_item_bg_desktop_lower_selector" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/mine_setting_item_height_desktop"/>
        <com.ss.android.lark.setting.SettingDividerView android:id="@+id/view_divider_schedule" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/setting_divider_height" style="@style/SettingDividerLeftBlank"/>
        <com.ss.android.lark.contact.impl.setting.widget.SwitchSettingView android:id="@+id/view_setting_document" android:background="@drawable/settings_item_bg_desktop_upper_selector" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/mine_setting_item_height_desktop"/>
        <com.ss.android.lark.setting.SettingDividerView android:id="@+id/view_divider_document" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/setting_divider_height" style="@style/SettingDividerLeftBlank"/>
        <com.ss.android.lark.contact.impl.setting.widget.SwitchSettingView android:id="@+id/view_setting_meeting" android:background="@drawable/settings_item_bg_desktop_lower_selector" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/mine_setting_item_height_desktop"/>
        <com.ss.android.lark.setting.SettingDividerView android:id="@+id/view_divider_meeting" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/setting_divider_height" style="@style/SettingDividerLeftBlank"/>
        <com.ss.android.lark.contact.impl.setting.widget.SwitchSettingView android:id="@+id/view_setting_mail" android:background="@drawable/settings_item_bg_desktop_upper_selector" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/mine_setting_item_height_desktop"/>
    </LinearLayout>
</LinearLayout>
