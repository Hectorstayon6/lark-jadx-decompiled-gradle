<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:padding="16dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <EditText android:textSize="14sp" android:textColor="@color/lkui_N900" android:id="@+id/idInputUrlEt" android:padding="8dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:hint="input url"/>
    <TextView android:textColor="@color/lkui_N00" android:gravity="center" android:id="@+id/idJumpTv" android:background="@drawable/inv_btn_blue_bg" android:padding="8dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:text="jump"/>
    <TextView android:textColor="@color/lkui_N00" android:gravity="center" android:id="@+id/idShareContainer" android:background="@drawable/inv_btn_blue_bg" android:padding="8dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:text="showShareContainer"/>
    <TextView android:textColor="@color/lkui_N00" android:gravity="center" android:id="@+id/idToggleShareType" android:background="@drawable/inv_btn_blue_bg" android:padding="8dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="16dp"/>
    <TextView android:textColor="@color/lkui_N00" android:gravity="center" android:id="@+id/idShareTextSys" android:background="@drawable/inv_btn_blue_bg" android:padding="8dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:text="系统分享文本"/>
    <TextView android:textColor="@color/lkui_N00" android:gravity="center" android:id="@+id/idShareImageSys" android:background="@drawable/inv_btn_blue_bg" android:padding="8dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:text="系统分享图片"/>
</LinearLayout>
