            </com.bytedance.ee.bear.mindnote.setting.SettingNormalImageView>
            <com.bytedance.ee.bear.mindnote.setting.SettingNormalImageView android:id="@+id/tool_left_view_img" android:background="@drawable/mindnote_toolbar_subitem_none" style="@style/MindNoteStructureFrame">
