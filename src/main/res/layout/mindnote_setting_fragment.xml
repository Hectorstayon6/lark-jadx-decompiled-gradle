            <com.bytedance.ee.bear.mindnote.setting.SettingNormalImageView android:id="@+id/tool_down_view_img" android:background="@drawable/mindnote_toolbar_subitem_right" style="@style/MindNoteStructureFrame">
                <androidx.appcompat.widget.AppCompatImageView app:srcCompat="@drawable/ud_icon_mindmap_down_outlined" style="@style/MindNoteStructureImage"/>
            </com.bytedance.ee.bear.mindnote.setting.SettingNormalImageView>
        </LinearLayout>
    </LinearLayout>
</com.bytedance.ee.bear.widgets.panel.CommonActionPanelLayout>
