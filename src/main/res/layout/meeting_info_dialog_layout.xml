                <com.ss.android.vc.common.widget.WidthAdaptationTextView android:textSize="14dp" android:textColor="@color/text_caption" android:id="@+id/meeting_info_id_label" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxWidth="124dp" android:text="@string/View_N_MeetingId" android:lineHeight="20dp" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="0"/>
