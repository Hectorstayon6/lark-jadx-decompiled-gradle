<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/container" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:id="@+id/meetingInfoLayout" android:background="@color/bg_body" android:paddingBottom="16dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="17dp" android:textStyle="bold" android:textColor="@color/text_title" android:ellipsize="end" android:id="@+id/meeting_info_dialog_title" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="8dp" android:text="xxx&apos;s Video Meeting xxx&apos;s Video Meeting xxx&apos;s Video Meeting xxx&apos;s Video Meeting xxx&apos;s Video Meeting xxx&apos;s Video Meeting" android:maxLines="2" android:layout_marginStart="16dp" android:layout_marginEnd="16dp" android:lineHeight="24dp"/>
