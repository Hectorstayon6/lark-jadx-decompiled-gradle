                <androidx.constraintlayout.widget.Barrier android:id="@+id/barrier_1" android:layout_width="wrap_content" android:layout_height="wrap_content" app:barrierDirection="3" app:constraint_referenced_ids="meeting_info_id_label,meeting_info_meetingid"/>
                <com.ss.android.vc.common.widget.WidthAdaptationTextView android:textSize="14dp" android:textColor="@color/text_caption" android:id="@+id/meeting_info_link_label" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="12dp" android:maxWidth="124dp" android:text="@string/View_M_MeetingLink" android:lineHeight="20dp" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/barrier_1"/>
                <TextView android:textSize="14dp" android:textColor="@color/text_title" android:id="@+id/meeting_info_link" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginTop="12dp" android:textIsSelectable="true" android:layout_marginStart="12dp" android:lineHeight="20dp" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toEndOf="@+id/vertical_barrier" app:layout_constraintTop_toBottomOf="@+id/barrier_1"/>
                <androidx.constraintlayout.widget.Barrier android:id="@+id/barrier_2" android:layout_width="wrap_content" android:layout_height="wrap_content" app:barrierDirection="3" app:constraint_referenced_ids="meeting_info_link_label,meeting_info_link"/>
                <com.ss.android.vc.common.widget.WidthAdaptationTextView android:textSize="14dp" android:textColor="@color/text_caption" android:id="@+id/meeting_info_dial_in_label" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="12dp" android:maxWidth="124dp" android:text="@string/View_M_DialIn" android:lineHeight="20dp" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/barrier_2"/>
                <LinearLayout android:orientation="vertical" android:id="@+id/meeting_info_dial_in_content" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginTop="12dp" android:layout_marginStart="12dp" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toEndOf="@+id/vertical_barrier" app:layout_constraintTop_toBottomOf="@+id/barrier_2">
