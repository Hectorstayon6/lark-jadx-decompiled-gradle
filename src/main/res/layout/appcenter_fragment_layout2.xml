<?xml version="1.0" encoding="utf-8"?>
<com.ss.android.appcenter.business.activity.appsort.TouchDetectFrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/workplace_fragment_container" android:background="@color/bg_body" android:layout_width="match_parent" android:layout_height="match_parent">
