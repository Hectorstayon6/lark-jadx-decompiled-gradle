<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" style="@style/user_profile_item_style">
    <TextView android:id="@+id/mAliasTypeNameTv" style="@style/user_profile_type_style"/>
    <ImageView android:id="@+id/mJumpIcon" android:layout_width="13dp" android:layout_height="13dp" android:src="0x7f0810d0" android:tint="@color/icon_n3" android:layout_centerVertical="true" android:layout_alignParentEnd="true"/>
    <com.ss.android.lark.ui.ExpandTextView android:id="@+id/mAliasValueTv" android:text="@string/Lark_Legacy_EditAlias" android:layout_toLeftOf="@+id/mJumpIcon" android:layout_centerVertical="true" android:layout_alignWithParentIfMissing="true" app:animDuration="100" app:arrowAlign="2" app:arrowPadding="0dp" app:arrowPosition="0" app:maxCollapsedLines="1" style="@style/user_profile_item_text_value_style"/>
