<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="303dp" android:layout_height="146dp">
    <LinearLayout android:orientation="vertical" android:id="@+id/oops_layout" android:background="@drawable/drive_oops_bg" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="17sp" android:gravity="center" android:id="@+id/uploadlist_oops_title" android:layout_marginTop="24dp" style="@style/Drive_CancelDialopgTextStyle"/>
        <TextView android:id="@+id/uploadlist_oops_message" style="@style/Drive_CancelDialogAlertTextStyle"/>
        <View android:id="@+id/divider" android:background="@color/line_border_card" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginTop="24dp"/>
        <LinearLayout android:layout_gravity="center" android:orientation="horizontal" android:id="@+id/uploadlist_button_layout" android:layout_width="match_parent" android:layout_height="50dp">
            <TextView android:textSize="17sp" android:textColor="@color/primary_pri_500" android:gravity="center" android:id="@+id/uploadlist_button_positive" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
            <View android:background="@color/line_border_card" android:layout_width="1dp" android:layout_height="match_parent"/>
            <TextView android:textSize="17sp" android:textColor="@color/primary_pri_500" android:gravity="center" android:id="@+id/uploadlist_button_negative" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
