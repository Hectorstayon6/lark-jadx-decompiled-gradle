<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/title_content_layout" android:paddingLeft="8dp" android:paddingTop="6dp" android:paddingBottom="6dp" android:layout_width="match_parent" android:layout_height="60dp">
        <com.larksuite.component.ui.imageview.LKUIRoundedImageView android:id="@+id/avatar" android:padding="2dp" android:layout_width="40dp" android:layout_height="40dp" android:layout_marginLeft="6dp" android:layout_marginTop="6dp" app:civ_oval="true" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintTop_toTopOf="0"/>
        <View android:id="@+id/avatar_click_wrapper" android:layout_width="48dp" android:layout_height="48dp"/>
        <ImageView android:id="@+id/update_tip" android:background="@drawable/main_title_disturb_tip_bg" android:visibility="gone" android:layout_width="16dp" android:layout_height="16dp" app:layout_constraintLeft_toRightOf="@+id/avatar" app:layout_constraintRight_toRightOf="@+id/avatar" app:layout_constraintTop_toTopOf="0"/>
        <ImageView android:id="@+id/reward_tip_view" android:background="@drawable/main_title_disturb_tip_bg" android:padding="2dp" android:visibility="gone" android:layout_width="16dp" android:layout_height="16dp" android:backgroundTint="@color/ud_O500" app:layout_constraintLeft_toRightOf="@+id/avatar" app:layout_constraintRight_toRightOf="@+id/avatar" app:layout_constraintTop_toTopOf="0"/>
        <ImageView android:id="@+id/bottom_tip" android:visibility="gone" android:layout_width="16dp" android:layout_height="16dp" app:layout_constraintBottom_toBottomOf="@+id/avatar" app:layout_constraintRight_toRightOf="@+id/avatar"/>
        <com.larksuite.component.ui.imageview.LKUIBadgeView android:id="@+id/new_user_tip" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" app:badgeBackground="@drawable/main_avatar_badge_new_user_red_dot" app:layout_constraintCircle="@+id/avatar" app:layout_constraintCircleAngle="45" app:layout_constraintCircleRadius="18dp"/>
        <ImageView android:id="@+id/tenant_avatar_badge" android:background="@drawable/main_avatar_badge_new_user_red_dot" android:visibility="gone" android:layout_width="10dp" android:layout_height="10dp" app:layout_constraintCircle="@+id/avatar" app:layout_constraintCircleAngle="45" app:layout_constraintCircleRadius="18dp"/>
        <com.ss.android.lark.main.app.title.LabelView android:textStyle="bold" android:textColor="@color/text_title" android:gravity="center" android:id="@+id/label" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="0dp" android:layout_marginLeft="8dp" app:layout_constraintBottom_toBottomOf="@+id/avatar" app:layout_constraintBottom_toTopOf="@+id/tenant_desc" app:layout_constraintHorizontal_bias="0" app:layout_constraintHorizontal_chainStyle="2" app:layout_constraintLeft_toRightOf="@+id/avatar" app:layout_constraintRight_toLeftOf="@+id/function_btn_container" app:layout_constraintTop_toTopOf="@+id/avatar" app:layout_constraintWidth_default="1"/>
        <View android:id="@+id/label_click_wrapper" android:layout_width="0dp" android:layout_height="48dp" app:layout_constraintLeft_toLeftOf="@+id/label" app:layout_constraintRight_toRightOf="@+id/label"/>
        <androidx.constraintlayout.widget.Group android:id="@+id/custom_status_layout" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" app:constraint_referenced_ids="custom_status_icon, custom_status_title, custom_status_right_icon"/>
        <ImageView android:id="@+id/custom_status_icon" android:layout_width="16dp" android:layout_height="16dp" android:layout_marginLeft="8dp" app:layout_constraintBottom_toBottomOf="@+id/label" app:layout_constraintHorizontal_bias="0" app:layout_constraintHorizontal_chainStyle="2" app:layout_constraintLeft_toRightOf="@+id/label" app:layout_constraintTop_toTopOf="@+id/label"/>
        <TextView android:textSize="12sp" android:textColor="@color/text_placeholder" android:ellipsize="end" android:id="@+id/custom_status_title" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="3dp" android:lines="1" app:layout_constraintBottom_toBottomOf="@+id/label" app:layout_constraintHorizontal_bias="0" app:layout_constraintHorizontal_chainStyle="2" app:layout_constraintLeft_toRightOf="@+id/custom_status_icon" app:layout_constraintRight_toLeftOf="@+id/custom_status_right_icon" app:layout_constraintTop_toTopOf="@+id/label" app:layout_constraintWidth_default="1"/>
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/custom_status_right_icon" android:layout_width="10dp" android:layout_height="10dp" android:layout_marginLeft="3dp" android:layout_marginRight="16dp" app:layout_constraintBottom_toBottomOf="@+id/label" app:layout_constraintHorizontal_bias="0" app:layout_constraintHorizontal_chainStyle="2" app:layout_constraintLeft_toRightOf="@+id/custom_status_title" app:layout_constraintRight_toLeftOf="@+id/function_btn_container" app:layout_constraintTop_toTopOf="@+id/label" app:srcCompat="@drawable/ud_icon_right_outlined" app:tint="@color/icon_n3"/>
        <TextView android:textSize="10sp" android:textColor="@color/text_placeholder" android:ellipsize="end" android:id="@+id/tenant_desc" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="2dp" android:maxWidth="150dp" android:maxLines="1" android:includeFontPadding="false" app:layout_constraintBottom_toBottomOf="@+id/avatar" app:layout_constraintHorizontal_bias="0" app:layout_constraintLeft_toLeftOf="@+id/label" app:layout_constraintRight_toLeftOf="@+id/function_btn_container" app:layout_constraintTop_toBottomOf="@+id/label"/>
        <androidx.constraintlayout.widget.Barrier android:id="@+id/tenant_barrier" android:layout_width="wrap_content" android:layout_height="wrap_content" app:barrierDirection="1" app:constraint_referenced_ids="tenant_desc,label"/>
        <View android:id="@+id/tenant" android:layout_width="1px" android:layout_height="1px" app:layout_constraintLeft_toRightOf="@+id/tenant_barrier"/>
        <androidx.constraintlayout.widget.ConstraintLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/function_btn_container" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginEnd="11dp" app:layout_constraintBottom_toBottomOf="@+id/avatar" app:layout_constraintEnd_toEndOf="0" app:layout_constraintHorizontal_bias="1" app:layout_constraintTop_toTopOf="@+id/avatar"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</merge>
