<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="vertical" android:background="@color/bg_body" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.ss.android.lark.ui.CommonTitleBar android:id="@+id/titlebar" android:layout_width="match_parent" android:layout_height="wrap_content" app:tb_title="@string/View_M_VideoMeetingSettings" app:tb_title_color="@color/text_title"/>
    <ScrollView android:background="@color/bg_base" android:layout_width="match_parent" android:layout_height="match_parent">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
            <include android:id="@+id/security_level_container" android:layout_width="match_parent" android:layout_height="wrap_content" layout="@layout/view_meeting_pre_setting_security_level"/>
            <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/container_1" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="8dp">
                <View android:id="@+id/top_divide_line" android:background="@color/line_divider_default" android:layout_width="match_parent" android:layout_height="0.5dp" app:layout_constraintTop_toTopOf="0"/>
                <View android:id="@+id/bottom_divide_line" android:background="@color/line_divider_default" android:layout_width="match_parent" android:layout_height="0.5dp" app:layout_constraintBottom_toBottomOf="0"/>
                <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/lobby_setting" android:layout_height="wrap_content" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/top_divide_line" style="@style/VcSettingBoxStyle">
                    <TextView android:id="@+id/lobby_setting_label" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_16" android:layout_marginTop="@dimen/dp_14" android:layout_marginRight="@dimen/dp_12" android:layout_marginBottom="@dimen/dp_14" android:text="@string/View_G_PlaceInLobby" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toLeftOf="@+id/lobby_setting_switcher" app:layout_constraintTop_toTopOf="0" style="@style/VcSettingTextStyle"/>
                    <com.ss.android.lark.widget.switch_button.SwitchButton android:id="@+id/lobby_setting_switcher" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="16dp" android:checked="true" app:layout_constraintBottom_toBottomOf="@+id/lobby_setting_label" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="@+id/lobby_setting_label" style="@style/LKUI_SwitchButton"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <View android:id="@+id/divide_line" android:background="@color/line_divider_default" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_marginLeft="16dp" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/lobby_setting"/>
                <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/pre_join_meeting_setting" android:layout_height="wrap_content" app:layout_constraintBottom_toTopOf="@+id/bottom_divide_line" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/divide_line" style="@style/VcSettingBoxStyle">
                    <TextView android:id="@+id/pre_join_meeting_setting_label" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_16" android:layout_marginTop="@dimen/dp_14" android:layout_marginRight="@dimen/dp_12" android:layout_marginBottom="@dimen/dp_14" android:text="@string/View_G_AllowJoinBeforeOrganizer" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toLeftOf="@+id/play_chimes_switcher" app:layout_constraintTop_toTopOf="0" style="@style/VcSettingTextStyle"/>
                    <com.ss.android.lark.widget.switch_button.SwitchButton android:id="@+id/pre_join_meeting_setting_switcher" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="16dp" android:checked="true" app:layout_constraintBottom_toBottomOf="@+id/pre_join_meeting_setting_label" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="@+id/pre_join_meeting_setting_label" style="@style/LKUI_SwitchButton"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout android:background="@color/bg_base" android:layout_width="match_parent" android:layout_height="match_parent">
                <TextView android:textColor="@color/text_placeholder" android:gravity="center_vertical" android:id="@+id/microphone_setting_title" android:layout_width="0dp" android:layout_height="30dp" android:layout_marginLeft="@dimen/dp_16" android:text="@string/View_M_ParticipantMicrophoneSettings" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0"/>
                <View android:id="@+id/microphone_top_divide_line" android:background="@color/line_divider_default" android:layout_width="match_parent" android:layout_height="0.5dp" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/microphone_setting_title"/>
                <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/microphone_setting" android:layout_height="wrap_content" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/microphone_top_divide_line" style="@style/VcSettingBoxStyle">
                    <TextView android:id="@+id/microphone_setting_label" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_16" android:layout_marginTop="@dimen/dp_14" android:layout_marginRight="@dimen/dp_12" android:layout_marginBottom="@dimen/dp_14" android:text="@string/View_M_MuteOnEntry" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toLeftOf="@+id/subtitle_switcher" app:layout_constraintTop_toTopOf="0" style="@style/VcSettingTextStyle"/>
                    <com.ss.android.lark.widget.switch_button.SwitchButton android:id="@+id/microphone_setting_switcher" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="16dp" android:checked="true" app:layout_constraintBottom_toBottomOf="@+id/microphone_setting_label" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="@+id/microphone_setting_label" style="@style/LKUI_SwitchButton"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <View android:id="@+id/microphone_bottom_divide_line" android:background="@color/line_divider_default" android:layout_width="match_parent" android:layout_height="0.5dp" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/microphone_setting"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/auto_record_layout" android:background="@color/bg_base" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent">
                <TextView android:textColor="@color/text_placeholder" android:gravity="center_vertical" android:id="@+id/record_settings_title" android:layout_width="0dp" android:layout_height="30dp" android:layout_marginLeft="@dimen/dp_16" android:text="@string/View_G_RecordingSettings" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0"/>
                <View android:id="@+id/record_settings_divider" android:background="@color/line_divider_default" android:layout_width="match_parent" android:layout_height="0.5dp" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/record_settings_title"/>
                <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/record_settings_switcher_container" android:layout_height="wrap_content" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/record_settings_divider" style="@style/VcSettingBoxStyle">
                    <TextView android:id="@+id/record_settings_txt" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_16" android:layout_marginTop="@dimen/dp_14" android:layout_marginRight="@dimen/dp_12" android:layout_marginBottom="@dimen/dp_14" android:text="@string/View_G_RecordMeetingAutomatically" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toLeftOf="@+id/record_settings_switcher" app:layout_constraintTop_toTopOf="0" style="@style/VcSettingTextStyle"/>
                    <com.ss.android.lark.widget.switch_button.SwitchButton android:id="@+id/record_settings_switcher" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="16dp" android:checked="false" app:layout_constraintBottom_toBottomOf="@+id/record_settings_txt" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="@+id/record_settings_txt" style="@style/LKUI_SwitchButton"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <View android:id="@+id/record_settings_bottom_divide_line" android:background="@color/line_divider_default" android:layout_width="match_parent" android:layout_height="0.5dp" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/record_settings_switcher_container"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
