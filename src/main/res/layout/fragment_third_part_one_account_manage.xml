            <TextView android:textSize="@dimen/dp_16" android:textColor="@color/text_title" android:gravity="center_vertical" android:id="@+id/tvAccount" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1" android:layout_marginStart="@dimen/dp_12" android:layout_marginEnd="@dimen/dp_12"/>
            <com.larksuite.component.universe_design.udswitch.UDSwitch android:id="@+id/switchBtn" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/dp_16"/>
