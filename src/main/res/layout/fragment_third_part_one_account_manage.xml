        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/container" android:background="@drawable/calendar_rectangle_white_r10" android:layout_width="match_parent" android:layout_height="@dimen/dp_52" android:layout_marginStart="@dimen/dp_16" android:layout_marginEnd="@dimen/dp_16">
