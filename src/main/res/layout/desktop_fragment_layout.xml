<?xml version="1.0" encoding="utf-8"?>
<com.ss.android.lark.main.app.widgets.MainDropRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/root" android:layout_width="match_parent" android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/left_side" android:background="@drawable/main_tab_bg" android:layout_width="60dp" android:layout_height="match_parent" android:layout_alignParentStart="true">
