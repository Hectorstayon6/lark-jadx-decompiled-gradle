        <com.ss.android.lark.main.app.tab.MainTab android:id="@+id/main_tab" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintBottom_toTopOf="@+id/tenant_switcher" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/main_avatar" app:layout_constraintVertical_bias="0"/>
