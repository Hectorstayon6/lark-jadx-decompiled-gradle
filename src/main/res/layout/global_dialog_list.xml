<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:id="@+id/group_dialog_list" android:background="@drawable/widget_common_color_white_conner6_bg" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:id="@+id/Linea_d" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textSize="14sp" android:textColor="@color/text_placeholder" android:gravity="center" android:id="@+id/title_dialog" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="24dp" android:layout_margin="12dp" android:maxLines="1"/>
            <TextView android:id="@+id/divider_title" android:background="@color/line_border_card" android:visibility="gone" android:layout_width="match_parent" android:layout_height="0.5dp"/>
        </LinearLayout>
        <ListView android:id="@+id/list_dialog" android:scrollbars="none" android:fadingEdge="none" android:layout_width="match_parent" android:layout_height="wrap_content" android:listSelector="@android:color/transparent" android:divider="@null" android:dividerHeight="0dp" android:overScrollMode="never"/>
    </LinearLayout>
    <FrameLayout android:id="@+id/menu_cancel_item" android:background="@drawable/widget_common_color_white_conner6_bg" android:visibility="gone" android:layout_width="match_parent" android:layout_height="66dp" android:layout_marginTop="10dp">
        <TextView android:textSize="17sp" android:textColor="@color/text_title" android:gravity="center" android:background="@drawable/dialog_list_cancel_item_selector" android:layout_width="match_parent" android:layout_height="match_parent" android:text="@string/Lark_Legacy_Cancel"/>
    </FrameLayout>
</LinearLayout>
