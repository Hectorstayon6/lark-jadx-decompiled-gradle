<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="horizontal" android:clipChildren="false" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="wrap_content">
    <FrameLayout android:id="@+id/title_click_wrapper" android:paddingLeft="12dp" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <FrameLayout android:id="@+id/title_press_wrapper" android:background="@drawable/workplace_header_title_bg" android:paddingLeft="4dp" android:paddingRight="4dp" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <LinearLayout android:orientation="horizontal" android:id="@+id/title_content_wrapper" android:clipChildren="false" android:clipToPadding="false" android:layout_width="wrap_content" android:layout_height="28dp">
                <com.larksuite.component.ui.imageview.LKUIRoundedImageView2 android:layout_gravity="center_vertical" android:id="@+id/title_icon" android:visibility="gone" android:layout_width="20dp" android:layout_height="20dp" android:layout_marginRight="8dp" app:border_color="@color/workplace_common_icon_border_color" app:border_width="@dimen/workplace_common_icon_border_width" app:radius="@dimen/workplace_small_icon_radius" app:tint="@color/fill_img_mask" app:tintMode="src_over"/>
                <com.larksuite.component.ignition.widget.squircle.SquircleImageView android:layout_gravity="center_vertical" android:id="@+id/title_icon_squircle" android:visibility="gone" android:layout_width="20dp" android:layout_height="20dp" android:layout_marginRight="8dp" app:border_color="@color/workplace_common_icon_border_color" app:border_width="@dimen/workplace_small_icon_border_width" app:radius="@dimen/workplace_small_icon_radius" app:tint="@color/fill_img_mask" app:tintMode="src_over"/>
                <TextView android:textSize="16dp" android:textStyle="bold" android:textColor="@color/text_title" android:ellipsize="end" android:gravity="left|center_vertical|center_horizontal|center" android:layout_gravity="center_vertical" android:id="@+id/header_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="1" android:maxEms="14"/>
                <com.larksuite.component.ui.imageview.LKUIRoundedImageView2 android:layout_gravity="center_vertical" android:id="@+id/arrow_icon" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:layout_marginLeft="2dp" android:src="@drawable/ud_icon_right_bold_outlined" app:tint="@color/icon_n3"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
    <FrameLayout android:id="@+id/internal_click_wrapper" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_weight="1"/>
    <FrameLayout android:id="@+id/menu_click_wrapper" android:paddingRight="14dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="12dp">
        <FrameLayout android:layout_gravity="center_vertical" android:paddingLeft="6dp" android:layout_width="wrap_content" android:layout_height="28dp">
            <LinearLayout android:layout_gravity="center_vertical" android:orientation="horizontal" android:id="@+id/menu_press_wrapper" android:background="@drawable/workplace_header_title_small_bg" android:padding="2dp" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <androidx.appcompat.widget.AppCompatImageView android:layout_gravity="center_vertical" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/ud_icon_more_outlined" app:tint="@color/icon_n3"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</LinearLayout>
