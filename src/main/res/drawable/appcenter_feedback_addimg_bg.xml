<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="4dp"/>
            <stroke android:width="1dp" android:color="@color/primary_pri_500"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/bg_body_overlay"/>
            <corners android:radius="4dp"/>
            <stroke android:width="1dp" android:color="@color/bg_filler"/>
        </shape>
    </item>
</selector>
