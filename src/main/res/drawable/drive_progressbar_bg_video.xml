<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="50dp"/>
            <solid android:color="@color/udtoken_tag_neutral_bg_inverse" android:alpha="0.25"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="50dp"/>
                <solid android:color="@color/primary_pri_500"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="50dp"/>
                <solid android:color="@color/udtoken_tag_neutral_bg_inverse" android:alpha="0.5"/>
            </shape>
        </clip>
    </item>
</layer-list>
