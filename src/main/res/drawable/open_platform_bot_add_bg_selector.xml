<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/opb_common_radius"/>
            <stroke android:width="1dp" android:color="@color/primary_pri_500"/>
            <solid android:color="@color/primary_pri_100"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/opb_common_radius"/>
            <stroke android:width="1dp" android:color="@color/primary_pri_500"/>
            <solid android:color="@color/bg_body"/>
        </shape>
    </item>
</selector>
