<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/udtoken_btn_se_bg_neutral_hover"/>
            <corners android:radius="6dp"/>
            <stroke android:width="1dp" android:color="@color/line_border_component"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/udtoken_component_outlined_bg"/>
            <corners android:radius="6dp"/>
            <stroke android:width="@dimen/dp_1" android:color="@color/line_border_component"/>
        </shape>
    </item>
</selector>
