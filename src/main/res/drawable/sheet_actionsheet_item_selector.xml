<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:tint="@color/fill_pressed" android:shape="rectangle">
            <corners android:radius="@dimen/space_radius_m"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:tint="@color/bg_filler" android:shape="rectangle">
            <corners android:radius="@dimen/ud_radius_l"/>
        </shape>
    </item>
    <item>
        <shape android:tint="@color/bg_body_overlay" android:shape="rectangle">
            <corners android:radius="@dimen/ud_radius_l"/>
        </shape>
    </item>
</selector>
