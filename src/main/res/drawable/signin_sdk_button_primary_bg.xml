<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/primary_pri_600"/>
            <corners android:radius="@dimen/signin_sdk_button_radius"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/fill_disable"/>
            <corners android:radius="@dimen/signin_sdk_button_radius"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/primary_pri_500"/>
            <corners android:radius="@dimen/signin_sdk_button_radius"/>
        </shape>
    </item>
</selector>
