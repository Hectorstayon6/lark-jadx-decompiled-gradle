<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="1dp"/>
            <solid android:color="@color/ud_B500_50"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="1dp"/>
                <solid android:color="@color/imtoken_message_voice_line_schedule_blue"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="1dp"/>
                <gradient android:startColor="@color/imtoken_message_voice_line_schedule_blue" android:endColor="@color/imtoken_message_voice_line_schedule_blue" android:angle="0" android:type="linear"/>
            </shape>
        </clip>
    </item>
</layer-list>
